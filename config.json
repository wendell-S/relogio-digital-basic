{
    "compilerOptions": {
      "target": "ES6",
      "module": "ESNext",
      "outDir": "assets/js",
      "rootDir": "assets/ts",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": [
      "assets/ts/**/*"
    ]
  }